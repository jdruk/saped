<!doctype html>
<html>
  <head>
    <meta charset='utf-8' />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <%= wicked_pdf_stylesheet_link_tag "pdf" -%>
    <%= wicked_pdf_javascript_include_tag "number_pages" %>
    
    <!--link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.2.10/semantic.css" integrity="sha256-8CEI/ltDBxfGb0lev/b4YiRbQSPlllT89ukpFKC3Frg=" crossorigin="anonymous" / -->
  </head>
  <body onload='number_pages'>
    <div id="content">
    </div>
     <h3>Lista geral de pagantes - Quantidade : <%= User.confirmeds.count %></h3>
        <table class="table">
            <thead>
                <tr>
                    <th>Nome</th>
                    <th>CPF</th>
                    <th>Filiação</th>
                </tr>
            </thead>
            <tbody>
                <% User.confirmeds.each do |user| %>
                    <tr>
                        <td><%= user.name.upcase %></td>
                        <td><%= user.cpf.upcase %></td>
                        <td><%= user.student_ufpi %></td>
                    </tr>
                <% end %>
            </tbody>
        </table>
  </body>
</html>
